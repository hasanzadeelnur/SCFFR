@{
}
@model List<TCYDMWebApp.DTO.RegionDTO>
@if (Model == null)
{
    return;
}
else
{
    @foreach (var item in Model)
    {
        if (Convert.ToInt32(Context.Request.Cookies["rId"]) == item?.Id)
        {
            <option selected value=@Convert.ToUInt32(item?.Id)>@item?.Name</option>
        }
        else
        {
            <option value=@Convert.ToUInt32(item?.Id)>@item?.Name</option>
        }

    }
}
